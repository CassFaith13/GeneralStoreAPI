    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>3d92d760-7399-4ce8-a239-c9b32b29fa5f</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="7.0.0">
